  "publisher": "Thinker",
  "license": "MIT",
  "icon": "images/mockserverlogo.png",
  "author": {
    "name": "Sivaraman",
    "email": "sendmsg2siva@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/R35007/mock-server-vscode-extension"
  },
  "files": [
    "out",
    "images",
    "samples",
    "schema"
  ],
  "keywords": [
